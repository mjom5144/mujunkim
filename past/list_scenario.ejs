<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    <% include ../../partials/head %>

</head>

<body>
  <% include ../../partials/commonjs %>
    <div id="wrapper">

        <!-- Navigation -->
        <% include ../../partials/nav %>

        <div id="page-wrapper">
            <div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header">시나리오 관리</h1>
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->

            <div class="row">
                <div class="col-lg-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            DataTables Advanced Tables
                        </div>
                        <!-- /.panel-heading -->
                        <div class="panel-body">
                            <table width="100%" class="table table-striped table-bordered table-hover" id="dataTables-example">
                                <thead>
                                    <tr>
                                        <th>시나리오 ID</th>
                                        <th>시나리오명</th>
                                        <th>시나리오 유형</th>
                                        <th>등록자</th>
                                        <th>개발자</th>
                                        <th>시나리오 개요</th>
                                    </tr>
                                </thead>
                                <tbody>
                                  <% data.forEach(function(e){ %>
                                      <tr>
                                        <td><a href="/scenarios/manage/detail/<%= e.ID %>"><%= e.ID %></a></td>
                                        <td><a href="/scenarios/manage/detail/<%= e.ID %>"><%= e.NAME %></td>
                                        <td><a href="/scenarios/manage/detail/<%= e.ID %>"><%= e.TYPE_CD %></td>
                                        <td><a href="/scenarios/manage/detail/<%= e.ID %>"><%= e.CREATE_ID %></td>
                                        <td><a href="/scenarios/manage/detail/<%= e.ID %>"><%= e.DEVELOPER_ID %></td>
                                        <td><a href="/scenarios/manage/detail/<%= e.ID %>"><%= e.SUMMARY %></td>
                                      </tr>
                                  <% }) %>
                                </tbody>
                            </table>
                            <br>
                            <a href="/scenarios/manage/register"><input type="button" value="신규"></a>
                        </div>
                        <!-- /.panel-body -->
                    </div>
                    <!-- /.panel -->
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->

        </div>
        <!-- /#page-wrapper -->

    </div>
    <!-- /#wrapper -->



    <!-- Page-Level Demo Scripts - Tables - Use for reference -->
    <script>
    $(document).ready(function() {
        $('#dataTables-example').DataTable({
            responsive: true
        });
    });
    </script>

</body>

</html>
